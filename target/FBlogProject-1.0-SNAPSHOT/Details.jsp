<%
    Blog blog = (Blog) request.getAttribute("blog");

%>

<div class="container">
    <div class="col-md-12">

        <div><h2 class="add"></h2></div>
    </div>
    <div class="col-md-12 ">

        <article><p>
            <%=blog.getTitle()%>
        </p></article>

        <article><p>
            <%=blog.getDetail()%>
        </p></article>
        <br>
        <hr>
    </div>
</div>

<body>

<!-- Optional JavaScript -->
